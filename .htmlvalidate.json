{
  "root": true,
  "extends": ["html-validate:recommended", "html-validate:document"],
  "rules": {
    "attr-pattern": "error",
    "class-pattern": "error",
    "id-pattern": "error",
    "name-pattern": "error",
    "prefer-native-element": ["error", { "exclude": ["button"] }],
    "long-title": "off",
    "require-sri": "off"
  }
}
